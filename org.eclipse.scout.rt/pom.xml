            </reports>
          </reportSet>
        </reportSets>
      </plugin>

      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-changelog-plugin</artifactId>
        <reportSets>
          <reportSet>
            <id>dual-report</id>
            <reports></reports><!-- Empty tag necessary with maven 3.6.1 because of bug https://issues.apache.org/jira/browse/MNG-6636 -->
            <configuration>
              <displayFileDetailUrl>https://github.com/eclipse-scout/scout.rt/blob/releases/10.0/%FILE%</displayFileDetailUrl>
              <displayFileRevDetailUrl>https://github.com/eclipse-scout/scout.rt/blob/%REV%/%FILE%</displayFileRevDetailUrl>
              <displayChangeSetDetailUrl>https://github.com/eclipse-scout/scout.rt/commit/%REV%</displayChangeSetDetailUrl>
            </configuration>
          </reportSet>
        </reportSets>
      </plugin>
    </plugins>
